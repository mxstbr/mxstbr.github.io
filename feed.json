---
---
{
    "version": "https://jsonfeed.org/version/1",
    "title": "Max Stoibers Blog",
    "home_page_url": "{{ site.url }}/",
    "feed_url": "{{ site.url }}/feed.json",
    "icon": "{{ site.url }}/favicon.png",
    "favicon": "{{ site.url }}/favicon.ico",
    "expired": false,
    "items": [
{% for post in site.posts %}
  {% if post.hidden != true %}
        {
            "id": "{{ post.id }}",
            "title": {{ post.title | smartify | jsonify }},
            "content_html": {{ post.content | jsonify }},
            "url": "{% if post.external_url %}{{ post.external_url }}{% else %}{{ site.url }}{{ post.url }}{% endif %}",
            "summary": {% if post.custom_excerpt %}{{ post.custom_excerpt | smartify | jsonify }}{% else %}{{ post.excerpt | smartify | jsonify }}{% endif %},
            "date_published": "{{ post.date }}"
        }{% unless forloop.last == true %},{% endunless %}
  {% endif %}
{% endfor %}
    ]
}